{
  "extends": ["airbnb-base", "prettier", "prettier/babel", "prettier/react", "plugin:cypress/recommended"],
  "plugins": ["react", "babel", "jsx-a11y", "prettier"],
  "globals": {
    "__DEV__": true,
    "__PROD__": true,
    "__TEST__": true,
    "wp": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error"],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  }
}
